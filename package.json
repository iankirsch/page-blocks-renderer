{
  "name": "page-blocks-renderer",
  "version": "1.1.9",
  "description": "Create web pages easily, using simple jsx builing block structures.",
  "main": "index.js",
  "scripts": {
    "dev": "npx ts-node .dev-env",
    "-start-esm": "node -r esm compiled",
    "-start": "node compiled",
    "-build-esm": "babel defaults -d compiled",
    "-build": "babel defaults -d compiled --plugins @babel/plugin-transform-modules-commonjs"
  },
  "author": "Ian Kirsch",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.7",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "colors": "^1.4.0",
    "esm": "^3.2.25",
    "jsdom": "^16.4.0",
    "node-watch": "^0.7.1",
    "fs-extra": "^9.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
